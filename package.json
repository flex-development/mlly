    "eslint": "8.39.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-chai-expect": "3.0.0",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-jsdoc": "40.1.2",
    "eslint-plugin-jsonc": "2.7.0",
