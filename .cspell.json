{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "allowCompoundWords": true,
  "caseSensitive": false,
  "dictionaries": ["@flex-development/mlly"],
  "dictionaryDefinitions": [
    {
      "name": "@flex-development/mlly",
      "path": "./.dictionary.txt"
    }
  ],
  "enableGlobDot": true,
  "failFast": false,
  "flagWords": [],
  "ignorePaths": [
    "**/*.snap",
    "**/.gitconfig",
    "**/.gitignore",
    "**/dist/",
    ".cspell.json",
    ".env*",
    ".eslintignore",
    ".git/",
    ".husky/_/",
    ".prettierignore",
    ".vercel/",
    ".vscode/settings.json",
    ".yarn/",
    "CHANGELOG.md",
    "LICENSE.md",
    "__tests__/report.json",
    "patches/",
    "tsconfig*temp.json",
    "vitest.config.*.timestamp-*.mjs",
    "yarn.lock"
  ],
  "ignoreRegExpList": [
    "/0x*/",
    "/@flex-development\\/.*/",
    "/from\\s+(['\"]).*\\1/",
    "^[^/]+\\/[^,;]+[^,]*?(?:;base64)?,(?:[\\S\\s]*)$",
    "import\\(.*\\)"
  ],
  "ignoreWords": [],
  "language": "en-US",
  "patterns": [],
  "readonly": true,
  "useGitignore": true,
  "usePnP": false,
  "version": "0.2",
  "words": ["fldv", "flexdevelopmentllc", "mlly"]
}
