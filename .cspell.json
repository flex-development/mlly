{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "allowCompoundWords": true,
  "caseSensitive": false,
  "dictionaries": ["@flex-development/mlly"],
  "dictionaryDefinitions": [
    {
      "name": "@flex-development/mlly",
      "path": "./.dictionary.txt"
    }
  ],
  "enableGlobDot": true,
  "failFast": false,
  "flagWords": [],
  "ignorePaths": [
    "**/*.snap",
    "**/.*ignore",
    "**/.gitconfig",
    "**/CHANGELOG.md",
    "**/LICENSE.md",
    "**/RELEASE_NOTES.md",
    ".cspell.json",
    ".git/",
    ".husky/_/",
    ".vscode/settings.json",
    ".yarn/",
    "patches/",
    "yarn.lock"
  ],
  "ignoreRegExpList": [
    "/0x*/",
    "/@flex-development\\/.*/",
    "/from\\s+(['\"]).*\\1/",
    "^[^/]+\\/[^,;]+[^,]*?(?:;base64)?,(?:[\\S\\s]*)$",
    "import\\(.*\\)"
  ],
  "ignoreWords": [],
  "language": "en-US",
  "patterns": [],
  "readonly": true,
  "useGitignore": true,
  "usePnP": false,
  "version": "0.2"
}
